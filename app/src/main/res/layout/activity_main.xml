<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="de.hottenstein.krimirundgang.MainActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/LatitudeText"
        android:layout_width="67dp"
        android:layout_height="16dp"
        android:layout_marginTop="16dp"
        android:text="Lat"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/LongitudeText" />

    <TextView
        android:id="@+id/LongitudeText"
        android:layout_width="67dp"
        android:layout_height="16dp"
        android:text="Lon"
        app:layout_constraintBaseline_toBaselineOf="@+id/LatitudeText"
        app:layout_constraintLeft_toRightOf="@+id/LatitudeText"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/button2"
        android:layout_width="201dp"
        android:layout_height="54dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:onClick="enterTourCycling"
        android:text="Touren"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/LatitudeText" />

</android.support.constraint.ConstraintLayout>
